{
  "name": "skanban",
  "version": "0.2.0",
  "description": "Simple Kanban tool",
  "homepage": "https://github.com/Sheryv/SKanban",
  "author": {
    "name": "Sheryv",
    "email": "sheryv.it@gmail.com"
  },
  "keywords": [
    "todo",
    "angular",
    "angular 10",
    "electron",
    "typescript",
    "sass"
  ],
  "main": "dist/index.html",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "npm run ng:serve",
    "ng:serve": "ng serve -c dev",
    "build:main": "cd main && npm run compile && cd ..",
    "build": "npm run build:main && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "build:web": "npm run build -- -c web-production",
    "electron:build:dev": "electron-builder build --publish=never --dir --config.asar=false",
    "electron:build": "electron-builder build --publish=never -w",
    "electron:build:single": "electron-builder build --publish=never -w portable",
    "release": "npm run build:prod && npm run electron:build:dev",
    "release-all": "npm run build:prod && npm run electron:build",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && playwright test -c e2e/playwright.config.ts e2e/",
    "e2e:show-trace": "playwright show-trace e2e/tracing/trace.zip",
    "build2": "npm run electron:compile-ts && ng build --base-href ./",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular-material-components/datetime-picker": "9.0.0",
    "@angular/animations": "^15.2.4",
    "@angular/cdk": "15.2.4",
    "@angular/common": "^15.2.4",
    "@angular/compiler": "^15.2.4",
    "@angular/core": "^15.2.4",
    "@angular/forms": "^15.2.4",
    "@angular/material": "15.2.4",
    "@angular/material-luxon-adapter": "15.2.4",
    "@angular/platform-browser": "^15.2.4",
    "@angular/platform-browser-dynamic": "^15.2.4",
    "@angular/router": "^15.2.4",
    "angular-split": "14.1.0",
    "dompurify": "3.0.1",
    "highlight.js": "11.8.0",
    "luxon": "3.3.0",
    "marked": "5.0.2",
    "marked-highlight": "2.0.1",
    "monaco-editor": "0.34.1",
    "ngx-color-picker": "14.0.0",
    "ngx-material-timepicker": "5.5.3",
    "ngx-monaco-editor-v2": "15.0.1",
    "rxjs": "7.8.0",
    "tslib": "~2.5.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.4",
    "@angular/compiler-cli": "^15.2.4",
    "@angular/language-service": "15.2.4",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "@playwright/test": "1.30.0",
    "@types/dompurify": "3.0.0",
    "@types/jasmine": "4.3.1",
    "@types/jasminewd2": "2.0.10",
    "@types/luxon": "3.2.0",
    "@types/marked": "4.0.8",
    "@types/node": "16.18.19",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "cross-env": "7.0.3",
    "electron-builder": "22.3.6",
    "eslint": "8.34.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "39.7.5",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jasmine-core": "4.5.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.1",
    "karma-coverage-istanbul-reporter": "3.0.3",
